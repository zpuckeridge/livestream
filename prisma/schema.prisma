generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model video {
  id          Int      @id @default(autoincrement())
  asset_id    String
  playback_id String
  title       String   @db.VarChar(255)
  description String   @db.VarChar(255)
  tag         String
  date        DateTime
  views       Int
  duration    Int
}
